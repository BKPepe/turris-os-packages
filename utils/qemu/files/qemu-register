#!/bin/sh /etc/rc.common

START=90

start() {
	mount -t binfmt_misc binfmt_misc /proc/sys/fs/binfmt_misc
	echo ':qemu-i386:M::\x7fELF\x01\x01\x01\x00\x00\x00\x00\x00\x00\x00\x00\x00\x02\x00\x03\x00:\xff\xff\xff\xff\xff\xfe\xfe\xff\xff\xff\xff\xff\xff\xff\xff\xff\xfe\xff\xff\xff:/usr/bin/qemu-i386:' > /proc/sys/fs/binfmt_misc/register
	echo ':qemu-x86-64:M::\x7fELF\x02\x01\x01\x00\x00\x00\x00\x00\x00\x00\x00\x00\x02\x00\x3e\x00:\xff\xff\xff\xff\xff\xfe\xfe\xff\xff\xff\xff\xff\xff\xff\xff\xff\xfe\xff\xff\xff:/usr/bin/qemu-x86_64:' > /proc/sys/fs/binfmt_misc/register
}

stop() { :; }
restart() { :; }
reload() { :; }
status() { return 0; }
